{
    "name": "kopaygorodsky/notification-bundle",
    "type": "symfony-bundle",
    "homepage": "https://github.com/kopaygorodsky/NotificationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vlad Kopaygorodsky",
            "email": "vlad.kopaygorodsky@gmail.com",
            "homepage": "https://github.com/kopaygorodsky"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^3.1",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/console": "^2.8|^3.0",
        "egulias/email-validator": "~2.0",
        "ramsey/uuid": "^3.7.1",
        "ratchet/pawl": "v0.3.1",
        "cboden/ratchet": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "symfony/phpunit-bridge": "~3.2",
        "symfony/yaml": "^2.8|^3.0",
        "friendsofphp/php-cs-fixer": "^1.1"
    },
    "suggest": {
        "jms/job-queue-bundle": "Required when using the default job provider.",
        "cboden/ratchet": "Create a websocket server",
        "lexik/jwt-authentication-bundle": "Jwt auth for websockets",
        "react/zmq": "sockets client"
    },
    "autoload": {
        "psr-4": { "Kopay\\NotificationBundle\\": ""}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
